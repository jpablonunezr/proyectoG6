<section class="user-login">
  <div class="cont-login">
    <span><%= alert %></span>
    <h2>Registro</h2>
    <h3>Plataforma de teachub</h3>
    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

    <%= f.email_field :email, autofocus: true, placeholder: 'EMAIL', autocomplete: "off" %>


    <%= f.password_field :password, placeholder: 'CLAVE'%>



    <%= f.password_field :password_confirmation, placeholder: 'REPITA CLAVE' %>
  


   <button type="submit">INGRESAR</button>
    <% end %>
  <%= render "devise/shared/links" %>
  <p>
  Lorem ipsum dolor sit amet, consectetur adipisicing elit. Officia expedita sit beata.
  </p>

  </div>
</section>

<section class="user-advise"></section>
