<p id="notice"><%= notice %></p>

<p>
  <strong>Nombre:</strong>
  <%= "#{@user.first_name}" "#{@user.last_name}" %>
</p>

<p>
  <strong>Email:</strong>
  <%= @user.email %>
</p>

<p>
  <strong>Foto:</strong>
  <%= @user.photo %>
</p>

<p>
  <strong>Nivel:</strong>
  <%= Level.find(@user.level_id).name %>
</p>

<p>
  <strong>Asignatura</strong>
  <%= Subject.find(@user.subject).name %>
</p>

<p>
  <strong>Materiales</strong>
  <% @user.materials.where(public_level: 1).each do |material| %>
  <tr>
    <td><%= material.title %></td>
    <td><%= material.description %></td>
  </tr>
  <% end %>
</p>

<%= link_to 'Back', materials_path %>
