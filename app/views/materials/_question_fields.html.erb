<div class="nested-fields">
	<fieldset>
	  <%= f.label :content, "Pregunta" %> <br />
	  <%= f.text_area :content %> <br />
	  <%#= f.check_box :_destroy %>
	  <%= link_to_remove_association "Eliminar Pregunta", f %>
	  <%#= f.label :_destroy, "Eliminar" %>
	  <%= f.fields_for :alternatives do |builder| %>
	      <%= render 'alternative_fields', f: builder %>
	  <%end %>
	  <div class="links">
    	<%= link_to_add_association "Agregar Alternativa", f, :alternatives %>
  	</div> 
	</fieldset>
</div>