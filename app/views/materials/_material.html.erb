<li id="materials_<%= material.id %>">
<a href="<%= edit_material_path(material) %>" class="a-frame">
	<h2> <%= material.title %></h2>
	<p><%= material.description %></p>
	<div><%= link_to '', material, method: :delete, data: { confirm: 'Seguro?' }, remote: true, class:'material-delete' %></div>
	<div class="material-subject subject-<%= material.subject.name %>"></div>
	<div class="public-level-<%= material.public_level %>"></div>
	</a>
</li>
